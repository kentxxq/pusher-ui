{
  "name": "pusher-ui",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "bak-build": "run-p type-check \"build-only {@}\" --",
    "bak-build-only": "vite build",
    "bak-type-check": "vue-tsc --build --force",
    "preview": "vite preview",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "format": "prettier --write src/",
    "build:development": "vue-tsc --build --force && vite build --mode development",
    "build:staging": "vue-tsc --build --force && vite build --mode staging",
    "build:production": "vue-tsc --build --force && vite build",
    "build": "vue-tsc --build --force && vite build"
  },
